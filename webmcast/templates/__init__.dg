# TODO
_error = subclass object where render =
    '''
    <!doctype html>
    <html>
        <head>
            <meta charset="ascii" />
            <title>{code}</title>
        </head>
        <body>
            <h1>error {code}</h1>
            <h3>{message}</h3>
        </body>
    </html>
    '''.format


load = name cache: {} ->
    name == 'error' => return _error
    raise FileNotFoundError name
